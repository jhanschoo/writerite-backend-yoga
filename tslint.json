{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "linterOptions": {
        "exclude": [
            "src/generated/**/*.ts"
        ]
    },
    "rules": {
      "quotemark": [true, "single"],
      "indent": [true, "spaces", 2],
      "ordered-imports": false,
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
